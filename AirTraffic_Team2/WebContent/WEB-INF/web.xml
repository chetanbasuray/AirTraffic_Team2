<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>AirTraffic_Team2</display-name>
  
  
  <servlet>
    <servlet-name>SearchFlightServlet</servlet-name>
    <servlet-class>com.AirTraffic.Team2.Servlets.SearchFlightServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SearchFlightServlet</servlet-name>
    <url-pattern>/searchFlight</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>BookFlightServlet</servlet-name>
    <servlet-class>com.AirTraffic.Team2.Servlets.BookFlightServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BookFlightServlet</servlet-name>
    <url-pattern>/booking</url-pattern>
  </servlet-mapping>
  
</web-app>